
package node;

/**
 *
 * @author Student-HSLH133
 */
class Node {
 
   private int value;
   Node head;
private Node curr;
   private int item;
  


public Node()
   {      
       value = 0;
      head = null;   
      Node n = new Node (new Integer(6));
      Node head = new Node(new Integer(9), n);
      
      
      
   }
   
   public Node(int newValue)
   {      
       value = newValue;       
   }   
   public Node(int newValue, Node newHead)
   {
       value = newValue;
       head = newHead;    
   }
      
   public int getValue()
   {       
       return value;      
   }
   
   public Node getHead()
   {            
      return head;       
   }
   public void setValue(int newValue)
   {
       value = newValue;
   }
   public void setNext(Node newHead)
   {
       head = newHead;
   }
   
  
   public static void main(String[] args) {
      LinkedString list = new LinkedString();
     
       
    }
    
}
